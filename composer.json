{
  "name": "ricardosierra/laravel-model-events",
  "description": "Laravel Custom Model Events",
  "keywords": [
    "laravel",
    "package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/igaster/laravel-model-events.git",
  "authors": [
    {
      "name": "Giannis Gasteratos",
      "email": "igasteratos@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/contracts": "5.* | ^6.0 | ^7.0 | ^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "~4.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Igaster\\ModelEvents\\": "src/",
      "Igaster\\ModelEvents\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Igaster\\ModelEvents\\ModelEventsServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
